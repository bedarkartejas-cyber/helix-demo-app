{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESnext",
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "baseUrl": ".",
        "outDir": "dist",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "jsx": "react-jsx",
        "allowSyntheticDefaultImports": true
    }
}
